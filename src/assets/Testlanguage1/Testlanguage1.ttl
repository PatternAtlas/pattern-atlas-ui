@prefix : <http://purl.org/patternpedia/Testlanguage1#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix pp: <http://purl.org/patternpedia#> .
@base <http://purl.org/patternpedia/Testlanguage1> .


<http://purl.org/patternpedia/Testlanguage1> rdf:type owl:Ontology ;
owl:imports <http://purl.org/patternpedia> ,
<http://purl.org/patternpedia/Testlanguage1/testpattern> .


# #################################################################
# #
# #    Sections / Data Properties
# #
# #################################################################


###  http://purl.org/patternpedia/cloudcomputingpatterns#hasContext
:hasContext rdf:type owl:DatatypeProperty .


###  http://purl.org/patternpedia/cloudcomputingpatterns#hasDrivingQuestion
:hasDrivingQuestion rdf:type owl:DatatypeProperty .


###  http://purl.org/patternpedia/cloudcomputingpatterns#hasExplanation
:hasExplanation rdf:type owl:DatatypeProperty .


###  http://purl.org/patternpedia/cloudcomputingpatterns#hasIcon
:hasIcon rdf:type owl:DatatypeProperty .


###  http://purl.org/patternpedia/cloudcomputingpatterns#hasIntent
:hasIntent rdf:type owl:DatatypeProperty .


###  http://purl.org/patternpedia/cloudcomputingpatterns#hasResult
:hasResult rdf:type owl:DatatypeProperty .


###  http://purl.org/patternpedia/cloudcomputingpatterns#hasSolution
:hasSolution rdf:type owl:DatatypeProperty .


###  http://purl.org/patternpedia/cloudcomputingpatterns#hasSolutionSketch
:hasSolutionSketch rdf:type owl:DatatypeProperty .


###  http://purl.org/patternpedia/cloudcomputingpatterns#hasVariation
:hasVariation rdf:type owl:DatatypeProperty .


# #################################################################
# #
# #    Restrictions / Classes
# #
# #################################################################
### http://purl.org/patternpedia/Testlanguage1 + 'Pattern'
:Testlanguage1 rdf:type owl:Class ;
 rdfs:subClassOf pp:Pattern ,
			[ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasSolutionSketch ;
                                                     owl:allValuesFrom xsd:anyURI
                                                   ] ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasVariation ;
                                                     owl:allValuesFrom xsd:string
                                                   ] ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasDrivingQuestion ;
                                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onDataRange xsd:string
                                                   ] ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasIcon ;
                                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onDataRange xsd:anyURI
                                                   ] ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasIntent ;
                                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onDataRange xsd:string
                                                   ] ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasContext ;
                                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onDataRange xsd:string
                                                   ] ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasResult ;
                                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onDataRange xsd:string
                                                   ] ,
                                                   [ rdf:type owl:Restriction ;
                                                     owl:onProperty :hasSolution ;
                                                     owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                                     owl:onDataRange xsd:string
                                                   ] .


#################################################################
#    Individuals
#################################################################

###  http://purl.org/patternpedia/Testlanguage1#Testlanguage1
:Testlanguage1 rdf:type owl:NamedIndividual ,
                                 pp:PatternLanguage ;
                        pp:containsPattern <http://purl.org/patternpedia/Testlanguage1/testpattern#testpattern> ;
                        pp:hasLogo "http://www.cloudcomputingpatterns.org/img/book.png"^^xsd:anyURI ;
                        pp:hasName "Testlanguage1"^^xsd:string  .


