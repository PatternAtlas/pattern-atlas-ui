@prefix : <http://purl.org/patternpedia/cloudcomputingpatterns2#> .
@prefix pp: <http://purl.org/patternpedia#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <http://purl.org/patternpedia/cloudcomputingpatterns2> .

<http://purl.org/patternpedia/cloudcomputingpatterns2> rdf:type owl:Ontology ;
                                                      owl:imports <http://purl.org/patternpedia>  .

#################################################################
#    Data properties
#################################################################

###  http://purl.org/patternpedia/cloudcomputingpatterns2#hasContext
:hasContext rdf:type owl:DatatypeProperty .


###  http://purl.org/patternpedia/cloudcomputingpatterns2#hasDrivingQuestion
:hasDrivingQuestion rdf:type owl:DatatypeProperty .


###  http://purl.org/patternpedia/cloudcomputingpatterns2#hasExplanation
:hasExplanation rdf:type owl:DatatypeProperty .


###  http://purl.org/patternpedia/cloudcomputingpatterns2#hasIcon
:hasIcon rdf:type owl:DatatypeProperty .


###  http://purl.org/patternpedia/cloudcomputingpatterns2#hasIntent
:hasIntent rdf:type owl:DatatypeProperty .


###  http://purl.org/patternpedia/cloudcomputingpatterns2#hasResult
:hasResult rdf:type owl:DatatypeProperty .


###  http://purl.org/patternpedia/cloudcomputingpatterns2#hasSolution
:hasSolution rdf:type owl:DatatypeProperty .





#################################################################
#    Classes
#################################################################

###  http://purl.org/patternpedia/cloudcomputingpatterns2#CloudComputingPattern
:cloudcomputingpatterns2 rdf:type owl:Class ;
                       rdfs:subClassOf pp:Pattern ,

                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasDrivingQuestion ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onDataRange xsd:string
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasIcon ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onDataRange xsd:anyURI
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasIntent ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onDataRange xsd:string
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasContext ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onDataRange xsd:string
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasResult ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onDataRange xsd:string
                                       ] ,
                                       [ rdf:type owl:Restriction ;
                                         owl:onProperty :hasSolution ;
                                         owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                                         owl:onDataRange xsd:string
                                       ] .



#################################################################
#    Individuals
#################################################################

###  http://purl.org/patternpedia/cloudcomputingpatterns2#cloudcomputingpatterns2
:CloudComputingPatterns2 rdf:type owl:NamedIndividual ,
                                 pp:PatternLanguage ;
                        pp:hasLogo "https://placekitten.com/300/200"^^xsd:anyURI ;
                        pp:hasName "Cloud Computing Patterns 2"^^xsd:string .

